<div class="container"> 
	<%= render_breadcrumbs :builder => ::BootstrapBreadcrumbsBuilder, :separator => "&raquo;" %>
	<div class="well well-lg center" >
		<% if flash[:notice] %>
			<div id="notice" class="alert alert-info"><%= notice %></div>
		<% end %>
		<h3><span class="label label-default">Visualização</span> Pesquisa interativa</h3>
		<br>	
		<p>
		  <strong>Titulo:</strong>
		  <%= @survey_form.title %>
		</p>

		<p>
		  <strong>Respostas:</strong>
		  <%= @survey_form.responses %>
		</p>
		<br>
		<p>
			<%= link_to "<button class='btn btn-danger' type='button'>Voltar</button>".html_safe, survey_forms_path %>
	        <%= link_to "<button class='btn btn btn-info' type='button'>Editar</button>".html_safe, edit_survey_form_path(@survey_form) %>
    	</p>
	</div>
</div>